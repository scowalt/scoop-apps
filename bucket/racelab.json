{
    "version": "3.5.0",
    "license": "Proprietary",
    "description": "Racelab is a third party overlay for iracing, Assetto Corsa Competizione, and Formula 1, that improves your race experience.",
    "homepage": "https://racelab.app",
    "architecture": {
        "64bit": {
            "url": "https://update.racelab.app/RacelabApps-3.5.0%20Setup.exe",
            "hash": "421380d63c9de498818bfbe0b1077987e9d756d426a12808b1aa9ed4f1148373",
            "installer": {
                
            },
            "uninstaller": {
                "script": "Invoke-Expression -Command \"$env:LOCALAPPDATA\\RacelabApps\\Update.exe --uninstall\""
            }
        }
    },
    "post_install": [
        "$null, $null2, $bucket = Get-Manifest($app)",
        "$bucketdir = Find-BucketDirectory($bucket)",
        "$bucketdir = $bucketdir.Trim()",
        "& \"$bucketdir\\scripts\\track.ps1\" install $app $version"
    ],
    "post_uninstall": [
        "$null, $null2, $bucket = Get-Manifest($app)",
        "$bucketdir = Find-BucketDirectory($bucket)",
        "$bucketdir = $bucketdir.Trim()",
        "& \"$bucketdir\\scripts\\track.ps1\" uninstall $app $version"
    ],
    "checkver": {
        "url": "https://update.racelab.app/latest.yml",
        "regex": "version: ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://update.racelab.app/RacelabApps-$version%20Setup.exe"
            }
        }
    }
}
